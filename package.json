{
  "name": "vault-image-analyzer",
  "version": "1.0.0",
  "description": "Complete image analysis system - Frontend (React) + Backend (Express + Python ML)",
  "main": "backend/server.js",
  "scripts": {
    "backend": "node backend/server.js",
    "frontend": "vite",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "start": "node backend/server.js",
    "setup": "npm install && pip install -r backend/requirements.txt",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "image-analysis",
    "ml",
    "forensics",
    "deep-learning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.487.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.12",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.12"
  }
}
